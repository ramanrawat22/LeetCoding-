//Largest element in an array//

class Solution {
    public static int largest(int n, int[] arr) {
        // code here
        int max = arr[0];
         
         for(int i=0; i<n; i++){
             
            if(arr[i]> max){
                max=arr[i];
                
            }
            
    
         }
         return max;
    }
}
